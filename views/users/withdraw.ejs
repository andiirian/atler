    <!-- Main content -->
    <section class="content">
      <!-- Small boxes (Stat box) -->
      <div class="row">
        <div class="col-md-6">
          <!-- general form elements -->
       <div class="box box-primary">
         <div class="box-header with-border">
           <h3 class="box-title">Quick Example</h3>
         </div>
         <!-- /.box-header -->
         <!-- form start -->
         <form role="form">
           <div class="box-body">
                 
             <div class="form-group">
               <label for="exampleInputEmail1">No.Rek</label>
               <input disabled type="text" class="form-control" id="exampleInputEmail1" placeholder="No.Rek">
             </div>
             
            
             
           </div>
           <!-- /.box-body -->

         
         </form>
       </div>
     </div>
        <div class="col-md-6">
             <!-- general form elements -->
          <div class="box box-primary">
            <div class="box-header with-border">
              <h3 class="box-title">Quick Example</h3>
            </div>
            <!-- /.box-header -->
            <!-- form start -->
            <form role="form" action="" method="POST">
              <div class="box-body">
                    <div class="form-group">
                            <label>Bank</label>
                            <select name="bank" class="form-control">
                              <option>Pilih</option>
                              <option value="BCA">BCA</option>
                              <option value="BRI">BRI</option>
                              <option value="BNI">BNI</option>
                              
                            </select>
                          </div>
                <div class="form-group">
                  <label for="exampleInputEmail1">No.Rek</label>
                  <input name="norek" type="text" class="form-control" id="exampleInputEmail1" placeholder="No.Rek">
                </div>
                <div class="form-group">
                  <label for="exampleInputPassword1">Amount</label>
                  <input name="amount" type="number" class="form-control" id="exampleInputPassword1" placeholder="">
                </div>
               
                
              </div>
              <!-- /.box-body -->

              <div class="box-footer">
                <button type="submit" class="btn btn-primary">Submit</button>
              </div>
            </form>
          </div>
        </div>
        <!-- ./col -->
      </div>
      <!-- /.row -->
      <!-- Main row -->
      
      <!-- /.row (main row) -->

    </section>


    <!-- /.content -->
    <section class="content">
        <div class="row">
            <div class="col-xs-12">
                    <div class="box">
                            <div class="box-header">
                              <h3 class="box-title">Withdraw List</h3>
                            </div>
                            <!-- /.box-header -->
                            <div class="box-body">
                              <table id="example2" class="table table-bordered table-hover">
                                <thead>
                                <tr>
                                  <th>Trx Date</th>
                                  <th>Amount</th>
                                  <th>Transaction</th>
                                  <th>Status</th>
                                  
                                </tr>
                                </thead>
                                <tbody>
                                  <%result.forEach(function(items){%>
                                <tr>
                                  <td><%=items.date%></td>
                                  <td><%=items.withdraw%>
                                  </td>
                                  <td><%=items.id_trx%></td>
                                  <%if(items.status == 0){%>
                                    <td>Waiting Confirm For Admin</td>
                                    <%}else if(items.status == 1){%>
                                      <td>Sukses</td>
                                      <%}else if(items.status == 2){%>
                                        <td>Failed</td>
                                        <%}%>
                                </tr>
                                <%})%>
                                </tbody>
                                <tfoot>
                                        <tr>
                                          <th>Trx Date</th>
                                          <th>Amount</th>
                                          <th>Transaction</th>
                                          <th>Status</th>
                                              </tr>
                                </tfoot>
                              </table>
                            </div>
                            <!-- /.box-body -->
                          </div>
            </div>
        </div>
