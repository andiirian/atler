<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>AlphaTrade | Dashboard</title>
  <!-- Tell the browser to be responsive to screen width -->
  <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
  <!-- Bootstrap 3.3.7 -->
  <link rel="stylesheet" href="/index/bower_components/bootstrap/dist/css/bootstrap.min.css">
  <!-- Font Awesome -->
  <link rel="stylesheet" href="/index/bower_components/font-awesome/css/font-awesome.min.css">
  <!-- Ionicons -->
  <!-- Theme style -->
  <link rel="stylesheet" href="/index/dist/css/AdminLTE.min.css">
  <!-- AdminLTE Skins. Choose a skin from the css/skins
       folder instead of downloading all of them to reduce the load. -->
  <link rel="stylesheet" href="/index/dist/css/skins/_all-skins.min.css">
  <!-- Morris chart -->

  <!-- jvectormap -->

  <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
  <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
  <!--[if lt IE 9]>
  <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
  <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
  <![endif]-->

  <!-- Google Font -->
  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
</head>
<body class="hold-transition skin-blue sidebar-mini">
<div class="wrapper">

  <header class="main-header">
    <!-- Logo -->
    <a href="index2.html" class="logo">
      <!-- mini logo for sidebar mini 50x50 pixels -->
      <span class="logo-mini"><b>A</b>T</span>
      <!-- logo for regular state and mobile devices -->
      <span class="logo-lg"><b>Alpha</b>Trade</span>
    </a>
    <!-- Header Navbar: style can be found in header.less -->
    <nav class="navbar navbar-static-top">
      <!-- Sidebar toggle button-->
      <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
        <span class="sr-only">Toggle navigation</span>
      </a>

      
    </nav>
  </header>
  <!-- Left side column. contains the logo and sidebar -->
  <aside class="main-sidebar">
    <!-- sidebar: style can be found in sidebar.less -->
    <section class="sidebar">
      <!-- Sidebar user panel -->
      <div class="user-panel">
        <div class="pull-left image">
          <img src="/index/dist/img/avatar6.png" class="img-circle" alt="User Image">
        </div>
        <div class="pull-left info">
          <p><%=user%></p>
          <a href="#"><i class="fa fa-circle text-success"></i> Online</a>
        </div>
      </div>
      <!-- search form -->
     
      <!-- /.search form -->
      <!-- sidebar menu: : style can be found in sidebar.less -->
      <ul class="sidebar-menu" data-widget="tree">
          <li class="header">PERSONAL</li>
          <li class="">
            <a href="dashboard">
              <i class="fa fa-dashboard"></i> <span>Dashboard</span>
              
            </a>
          
          </li>
          <li>
              <a href="profil">
                <i class="fa fa-user"></i> <span>Profil</span>
                
              </a>
            </li>
            <li>
              <a href="referrals">
                <i class="fa fa-users"></i> <span>My Referrals</span>
                
              </a>
            </li>
          <li class="header">TRANSACTION</li>
          <li>
            <a href="deposit">
              <i class="fa fa-cubes"></i> <span>Deposits</span>
              
            </a>
          </li>
        
         
        
          <li>
            <a href="withdraw">
              <i class="fa fa-database"></i> <span>Withdraw</span>
            
            </a>
            
          </li>
          <li class="header">LIST</li>
          <li>
              <a href="investmentList">
                <i class="fa fa-table"></i> <span>Investment List</span>
              
              </a>
              
            </li>
            <li>
                <a href="withdrawList">
                  <i class="fa fa-table"></i> <span>Withdraw List</span>
                
                </a>
                
              </li>
              <li>
                  <a href="earningList">
                    <i class="fa fa-table"></i> <span>Earning List</span>
                  
                  </a>
                  
                </li>
              
    
          <li>
                  <a href="logout.user">
                    <i class="fa fa-sign-out"></i> <span>Logout</span>
                  
                  </a>
                  
                </li>
       
         
          
        </ul>
    </section>
    <!-- /.sidebar -->
  </aside>

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
        Dashboard
        <small>Control panel</small>
      </h1>
      <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
        <li class="active">Dashboard</li>
      </ol>
    </section>

    <!-- Main content -->
    <section class="content">
      <!-- Small boxes (Stat box) -->
      <div class="row">
        <div class="col-lg-3 col-xs-12">
          <!-- small box -->
          <div class="small-box bg-aqua">
            <div class="inner">
                <%if(total[3].dompet != null){%>
                  <h3>$ <%=total[3].dompet.toLocaleString()%>,00</h3>
                  <%}else{%>
                    <h3>$ ,00</h3>
                    <%}%>
              

              <p>Account Balance</p>
            </div>
            <div class="icon">
              <i class="fa fa-credit-card"></i>
            </div>
            <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
          </div>
        </div>
        <!-- ./col -->
        <div class="col-lg-3 col-xs-12">
          <!-- small box -->
          <div class="small-box bg-green">
            <div class="inner">
                <%if(total[1][0].investasi != null){%>
                  <h3>$ <%=total[1][0].investasi.toLocaleString()%>,00</h3>
                  <%}else{%>
                    <h3>$ 0,00</h3>
                    <%}%>
              
              

              <p>Total Invesment</p>
            </div>
            <div class="icon">
              <i class="fa fa-database"></i>
            </div>
            <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
          </div>
        </div>
        <!-- ./col -->
        <div class="col-lg-3 col-xs-12">
          <!-- small box -->
          <div class="small-box bg-yellow">
            <div class="inner">
                <%if(total[0][0].pendapatan != null){%>
                  <h3>$ <%=total[0][0].pendapatan.toLocaleString()%>,00</h3>
                  <%}else{%>
                    <h3>$ 0,00</h3>
                    <%}%>
           

              <p>Total Earnings</p>
            </div>
            <div class="icon">
              <i class="fa fa-dollar"></i>
            </div>
            <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
          </div>
        </div>
        <!-- ./col -->
        <div class="col-lg-3 col-xs-12">
          <!-- small box -->
          <div class="small-box bg-red">
            <div class="inner">
                <%if(total[2][0].withdraw != null){%>
                  <h3>$ <%=total[2][0].withdraw.toLocaleString()%>,00</h3>
                  <%}else{%>
                    <h3>$ 0,00</h3>
                    <%}%>
              

              <p>Total Withdraw</p>
            </div>
            <div class="icon">
              <i class="fa fa-database"></i>
            </div>
            <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
          </div>
        </div>
        <!-- ./col -->
      </div>
      <!-- /.row -->
      <!-- Main row -->
      
      <!-- /.row (main row) -->

    </section>

    <section class="content">
      <div class="row">
          <div class="col-xs-12">
                  <div class="box">
                          <div class="box-header">
                            <h3 class="box-title">Deposit List</h3>
                          </div>
                          <!-- /.box-header -->
                          <div class="box-body">
                            <table id="example2" class="table table-bordered table-hover">
                              <thead>
                              <tr>
                                <th>Tx Id</th>
                                <th>Next Profit</th>
                                <th>Expired Date</th>
                                <th>Plan</th>
                                <th>Status</th>
                              </tr>
                              </thead>
                              <tbody>
                                <%result[0].forEach(function(items){%>

                                
                              <tr>
                                
                                
                                
                                <%if(items.status == 0){%>
                                  <td><%=items.tx_id%></td>
                                  <td></td>
                                  <td></td>
                                  <td><%=items.investasi.toLocaleString()%></td>
                                  <td><a href="payment-confirm/<%=items.tx_id%>">Confirm</a></td>
                                  <%}else if(items.status == 1){%>
                                    <td><%=items.tx_id%></td>
                                    <td></td>
                                    <td></td>
                                    <td><%=items.investasi.toLocaleString()%></td>
                                    <td><span class="label label-warning">Waiting Confirm</span></td>
                                    <%}else if(items.status == 2){%>
                                      <td><%=items.tx_id%></td>
                                      <td><%=items.date%>
                                      </td>
                                      <td><%=items.jatuh_tempo%></td>
                                      <td><%=items.investasi.toLocaleString()%></td>
                                      <td><span class="label label-info">Running</span></td>
                                      <%}else if(items.status == 3){%>
                                        <td><%=items.tx_id%></td>
                                  <td></td>
                                  <td></td>
                                  <td><%=items.investasi.toLocaleString()%></td>
                                     <td> <span class="label label-danger">Failed</span></td>
                                        <%}else{%>
                                          <td><%=items.tx_id%></td>
                                          <td>Expired</td>

                                          <td>Expired</td>
                                          <td><%=items.investasi.toLocaleString()%></td>
                                          <td>Expired</td>
                                          <%}%>
                              </tr>
                              <%})%>
                             
                              
                              
                             
                              
                              
                              </tbody>
                              <tfoot>
                                      <tr>
                                              <th>Tx Id</th>
                                <th>Next Profit</th>
                                <th>Expired Date</th>
                                <th>Plan</th>
                                <th>Status</th>
                                            </tr>
                              </tfoot>
                            </table>
                          </div>
                          <!-- /.box-body -->
                        </div>
          </div>
      </div>
  </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->
  <footer class="main-footer">
    <div class="pull-right hidden-xs">
      <b>Version</b> 2.4.0
    </div>
    <strong>Copyright &copy; 2014-2016 <a href="https://adminlte.io">Almsaeed Studio</a>.</strong> All rights
    reserved.
  </footer>

  <!-- Control Sidebar -->

  <!-- /.control-sidebar -->
  <!-- Add the sidebar's background. This div must be placed
       immediately after the control sidebar -->
  <div class="control-sidebar-bg"></div>
</div>
<!-- ./wrapper -->

<script src="/index/bower_components/jquery/dist/jquery.min.js"></script>
<!-- Bootstrap 3.3.7 -->
<script src="/index/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
<!-- DataTables -->
<script src="/index/bower_components/datatables.net/js/jquery.dataTables.min.js"></script>
<script src="/index/bower_components/datatables.net-bs/js/dataTables.bootstrap.min.js"></script>
<!-- SlimScroll -->
<script src="/index/bower_components/jquery-slimscroll/jquery.slimscroll.min.js"></script>
<!-- FastClick -->
<script src="/index/bower_components/fastclick/lib/fastclick.js"></script>
<!-- AdminLTE App -->
<script src="/index/dist/js/adminlte.min.js"></script>
<!-- AdminLTE for demo purposes --> 
<script src="/index/dist/js/demo.js"></script>
<script>
        $(function () {
          $('#example1').DataTable()
          $('#example2').DataTable({
            'paging'      : true,
            'lengthChange': false,
            'searching'   : false,
            'ordering'    : true,
            'info'        : true,
            'autoWidth'   : false
          })
        })
      </script>
</body>
</html>
